public class MailException extends Exception {
	private static final long serialVersionUID = 6388464682177215737L;

	public MailException(String mensaje) {
		super(mensaje);
	}

	public MailException(String mensaje, Throwable throwable) {
		super(mensaje, throwable);
	}
}